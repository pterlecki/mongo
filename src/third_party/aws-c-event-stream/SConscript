# -*- mode: python -*-

Import('env')

env = env.Clone()

env.Prepend(CPPPATH=[
    'include',
])

env.InjectThirdParty(libraries=['aws-c-common','aws-checksums'])
env.Library(
    target= "aws-c-event-stream",
    source= [
        "source/event_stream.c",
    ],
    LIBDEPS=[
        '$BUILD_DIR/third_party/shim_aws_c_common',
        '$BUILD_DIR/third_party/shim_aws_checksums',
    ],
)
